/******** [END] Left panel -> navigation menu style definition ********/
.navbar {
	background: #272c33;
	border-radius: 0;
	border: none;
	margin: 0;
	margin-bottom: 100px;
	padding: 0
}

.navbar-expand-lg {
	flex-wrap: nowrap;
	justify-content: flex-start
}

.navbar .navbar-brand {
	border-bottom: 1px solid #4e4e52;
	color: #f1f2f7 !important;
	font-family: Open Sans;
	font-size: 22px;
	float: none;
	line-height: 50px;
	margin: 0;
	text-align: left;
	text-transform: capitalize;
	display: block;
	min-height: 69px;
	padding: 0;
	display: flex;
	justify-content: center;
	align-items: center;
	position: relative
}

.navbar .navbar-brand span {
	font-weight: 600
}

.navbar .navbar-brand img {
	max-width: 160px
}

.navbar .menu-title {
	border-bottom: 1px solid #4e4e52;
	color: #9496a1;
	clear: both;
	display: block;
	font-family: Open Sans;
	font-size: 14px;
	font-weight: 700;
	line-height: 50px;
	padding: 15px 0 0;
	text-transform: uppercase
}

.navbar .navbar-nav {
	float: none;
	position: relative
}

.navbar .navbar-nav li {
	width: 100%
}

.navbar .navbar-nav li.active .menu-icon, .navbar .navbar-nav li .toggle_nav_button.nav-open:before,
	.navbar .navbar-nav li:hover .toggle_nav_button:before {
	color: #fff !important
}

.navbar .navbar-nav li.active>a {
	padding: 15px 0 15px 35px
}

.navbar .navbar-nav li.active>a:before {
	display: none
}

.navbar .navbar-nav li.active>a:after {
	content: "";
	width: 0;
	height: 0;
	border-top: 7px solid transparent;
	border-right: 9px solid #f1f2f7;
	border-bottom: 7px solid transparent;
	position: absolute;
	top: 19px;
	right: 0
}

.navbar .navbar-nav li>a {
	background: none !important;
	color: #c8c9ce !important;
	display: inline-block;
	font-family: Open Sans;
	font-size: 14px;
	line-height: 30px;
	padding: 10px 0 10px 35px;
	position: relative;
	width: 100%
}

.navbar .navbar-nav li>a:hover, .navbar .navbar-nav li>a:hover .menu-icon
	{
	color: #fff !important
}

.navbar .navbar-nav li>a .menu-icon {
	color: #8b939b;
	position: absolute;
	top: 18px;
	left: 0;
	width: 60px;
	text-align: left;
	z-index: 9
}

.navbar .navbar-nav li>a .menu-title-text {
	font-size: 14px
}

.navbar .navbar-nav li>a .badge {
	border-radius: 0;
	font-family: Open Sans;
	font-weight: 600;
	float: right;
	margin: 6px 0 0;
	padding: .4em .5em
}

.navbar .navbar-nav li.menu-item-has-children {
	position: relative
}

.navbar .navbar-nav li.menu-item-has-children a {
	line-height: 30px;
	padding-left: 35px
}

.navbar .navbar-nav li.menu-item-has-children a:before {
	content: "\F105";
	color: #c8c9ce;
	font-family: Fontawesome;
	font-size: 16px;
	position: absolute;
	top: 10px;
	right: 20px;
	text-align: right;
	transition: all .25s ease
}

.navbar .navbar-nav li.menu-item-has-children a:hover:before {
	color: #fff
}

.navbar .navbar-nav li.menu-item-has-children .sub-menu {
	max-height: 0;
	overflow-y: hidden;
	opacity: 0;
	padding-left: 35px;
	transition: max-height .3s ease-in-out, opacity .25s ease
}

.navbar .navbar-nav li.menu-item-has-children .sub-menu a {
	padding: 5px 0 5px 30px
}

.navbar .navbar-nav li.menu-item-has-children .sub-menu a:before {
	content: "";
	display: none
}

.navbar .navbar-nav li.menu-item-has-children .sub-menu a .menu-icon {
	top: 13px;
	text-align: left;
	width: 25px
}

.navbar .navbar-nav li.menu-item-has-children.open a:before {
	content: "\F107"
}

.navbar .navbar-nav li.menu-item-has-children.open .sub-menu {
	max-height: 1000px;
	opacity: 1
}

.navbar .navbar-nav li.menu-item-has-children.open .open+.sub-menu {
	display: none
}

.navbar .navbar-nav li.menu-item-has-children a.open:before {
	content: "\F107"
}

.navbar .navbar-nav li.menu-item-has-children .open+.sub-menu {
	max-height: 1000px;
	opacity: 1
}

.navbar .navbar-nav>.active>a, .navbar .navbar-nav>.active>a:focus,
	.navbar .navbar-nav>.active>a:hover {
	background: #28282e !important;
	color: #d7d9e3 !important
}

.navbar-nav li span.count {
	background: #a9d86e;
	border-radius: 50%;
	color: #fff;
	font-family: Open Sans;
	font-size: 9px;
	font-weight: 700;
	float: right;
	height: 20px;
	width: 20px;
	line-height: 20px;
	margin-right: 15px;
	text-align: center
}


